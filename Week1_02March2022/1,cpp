#include <iostream>
using namespace std;
void solve(){
int n;
cin>>n;
int a[n];
for(int i=0;i<n;i++)cin>>a[i];
int key;
cin>>key;
bool ok=false;

    int comparisons=0;
    for(int i=0;i<n;i++)
    {
        comparisons++;
        if(a[i]==key){
            ok=true;
        break;
        }
    }

    if(ok){
        cout<<"Present "<<comparisons<<endl;
    }
    else cout<<"Not Present "<<comparisons<<endl;

}
int main(){
int t;cin>>t;
while(t--){

    solve();
}

return 0;
}
